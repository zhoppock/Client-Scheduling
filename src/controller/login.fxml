<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="589.0" prefWidth="985.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.login_control">
   <TextField fx:id="usernameInput" layoutX="418.0" layoutY="262.0" promptText="Username" />
   <PasswordField fx:id="password" layoutX="417.0" layoutY="315.0" promptText="Password" />
   <Button fx:id="loginTxt" layoutX="470.0" layoutY="391.0" mnemonicParsing="false" onAction="#goToMainMenu" text="Login" textAlignment="CENTER" />
   <Label fx:id="headerTxt" alignment="CENTER" layoutX="409.0" layoutY="88.0" text="Login to Proceed" textAlignment="CENTER" underline="true">
      <font>
         <Font size="22.0" />
      </font>
   </Label>
   <Label fx:id="locationTxt" layoutX="409.0" layoutY="181.0" text="Connecting from: ..." />
   <Button fx:id="exitTxt" layoutX="837.0" layoutY="46.0" mnemonicParsing="false" onAction="#exitApplication" text="Exit" />
</AnchorPane>
